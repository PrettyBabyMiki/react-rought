    "changeset": "changeset",
    "version": "changeset version",
    "postversion": "node scripts/postversion.mjs",
    "version:experimental": "node scripts ./scripts/version experimental",
    "watch": "rollup -c -w"
  },
  "jest": {
    "projects": [
      "<rootDir>/packages/*"
    ]
  },
