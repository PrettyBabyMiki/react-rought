{
  "name": "@remix-run/react-router",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/react-router",
      "packages/react-router-dom",
      "packages/react-router-dom-v5-compat",
      "packages/react-router-native",
      "packages/router"
    ],
